{
	"name": "ts.pt",
	"description": "A Typescript Project Template",
	"version": "1.1.0-alpha.9",
	"main": "dist/ts.pt.js",
	"module": "dist/ts.pt.esm.js",
	"types": "dist/ts.pt.d.ts",
	"homepage": "https://github.com/billowz/ts.pt",
	"author": "Billow Z <billowz@hotmail.com>",
	"bugs": {
		"url": "https://github.com/billowz/ts.pt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/billowz/ts.pt.git"
	},
	"keywords": [
		"Template",
		"Typescript",
		"Rollup",
		"general-release"
	],
	"files": [
		"dist",
		"doc",
		"CHANGELOG.md",
		"README.md",
		"LICENSE"
	],
	"private": false,
	"license": "MIT",
	"scripts": {
		"clean:dist": "rimraf dist",
		"clean:doc": "rimraf doc",
		"clean:cover": "rimraf coverage",
		"clean": "rimraf dist doc coverage",
		"lint": "tslint -c tslint.json ./src/**/*.ts",
		"check": "tsc --noEmit",
		"dts": "dts-bundle-generator -o dist/argilo.d.ts src/index.ts",
		"mocha": "mocha --opts mocha.opts src/**/*.spec.ts",
		"unit:node": "cross-env NODE_ENV=test nyc mocha src/**/*.spec.ts",
		"unit:browser": "karma start config/karma.config.js",
		"unit:sauce": "karma start config/karma.sauce.config.js",
		"compile": "rollup -c config/rollup.config.js --environment NODE_ENV:production",
		"compile:dev": "rollup -c config/rollup.config.js --environment NODE_ENV:development",
		"doc": "typedoc --options typedoc.json",
		"perf:browser": "karma start config/karma.perf.config.js",
		"package": "npm-run-all clean lint check dts compile doc"
	},
	"releaseConfig": {
		"config": "./config/release.yml"
	},
	"devDependencies": {
		"@types/benchmark": "^1.0.31",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.0.8",
		"benchmark": "^2.1.4",
		"console-polyfill": "^0.3.0",
		"cross-env": "^5.2.0",
		"dts-bundle-generator": "^3.0.0",
		"general-release": "^0.0.17",
		"jsdom": "^15.1.1",
		"jsdom-global": "^3.0.2",
		"json3": "^3.3.3",
		"karma": "^4.2.0",
		"karma-bench": "^0.1.3",
		"karma-benchmark-reporter": "^0.1.1",
		"karma-chrome-launcher": "^3.0.0",
		"karma-coverage-istanbul-reporter": "github:billowz/karma-coverage-istanbul-reporter#fix-source-finder",
		"karma-expect": "^1.1.3",
		"karma-firefox-launcher": "^1.1.0",
		"karma-ie-launcher": "^1.0.0",
		"karma-mocha": "^1.3.0",
		"karma-rollup": "^1.0.1",
		"karma-sauce-launcher": "^2.0.2",
		"karma-spec-reporter": "0.0.32",
		"mocha": "^2.5.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^14.1.1",
		"prettier": "^1.18.2",
		"rimraf": "^2.6.3",
		"rollup": "^1.17.0",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-istanbul": "github:billowz/rollup-plugin-istanbul#rc",
		"rollup-plugin-jscc": "github:billowz/rollup-plugin-jscc",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-progress": "^1.1.1",
		"rollup-plugin-terser": "^5.1.1",
		"rollup-plugin-typescript": "^1.0.1",
		"rollup-plugin-visualizer": "^2.5.0",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-sonarts": "^1.9.0",
		"typedoc": "^0.14.2",
		"typedoc-plugin-example-tag": "^1.0.2",
		"typedoc-plugin-external-module-name": "^2.1.0",
		"typescript": "^3.5.3"
	}
}
